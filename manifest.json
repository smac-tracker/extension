{
    "name": "Extension",
    "version": "1.0",
    "description": "Testing",
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "browser_action": { 
        "default_popup": "popup.html",
        "default_title": "bear"
    }
}